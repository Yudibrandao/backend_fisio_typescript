{
    "name": "backend_fisio",
    "version": "1.0.1",
    "description": "backend_fisio is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
       
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpileOnly ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "backend_fisio"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "helmet": "^3.21.2",
        "express-custom-error": "^1.8.4",
        "rotating-file-stream": "^1.4.6",
        "http-status": "^1.3.2",
        "has-keys": "^0.1.0",
        "morgan": "^1.9.1",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "@types/express": "^4.17.1",
        "ts-node-dev": "^1.0.0-pre.43",
        "typescript": "^3.6.3",
        "mysql2": "^1.7.0",
        "sequelize": "^5.19.2",
        "express": "^4.17.1"
    },
    "_moduleAliases": {
        "@root"      : "src",
        "@controllers" : "src/controllers",
        "@models" : "src/models",
        "@util" : "src/util",
        "@routes" : "src/routes"
    }
}