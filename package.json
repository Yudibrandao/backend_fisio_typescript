{
    "name": "backend_fisio",
    "version": "1.0.1",
    "description": "backend_fisio is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpileOnly ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "backend_fisio"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "jsonwebtoken": "^9.0.2",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.9.1",
        "mysql2": "^1.7.0",
        "rotating-file-stream": "^1.4.6",
        "sequelize": "^5.19.2",
        "ts-node-dev": "^1.0.0-pre.43"
    },
    "_moduleAliases": {
        "@root": "src",
        "@controllers": "src/controllers",
        "@models": "src/models",
        "@util": "src/util",
        "@routes": "src/routes"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "typescript": "^5.4.5"
    }
}
